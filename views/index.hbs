<a-scene embedded arjs="debugUIEnabled: false" vr-mode-ui="enabled: false">
  <a-assets>
    <img id="texture" src="/images/brick.jpg" />
    <img id="leaf" src="/images/leaf.jpg" />
    <img id="aoimg" src="/images/aologo.jpg" />
    <img id="happy_birthday" src="/images/happybirthday.png">
    <img id="happy_holiday" src="/images/happyholiday.png">
    <a-asset-item id="santa-obj" src="/images/santa.obj"></a-asset-item>
    <a-asset-item id="santa-mtl" src="/images/santa.mtl"></a-asset-item>
    <a-asset-item id="tree-obj" src="/images/tree.obj"></a-asset-item>
    <a-asset-item id="tree-mtl" src="/images/tree.mtl"></a-asset-item>
  </a-assets>>
  {{!-- <a-marker-camera preset="hiro"> --}}
  <a-marker preset="hiro">
    <a-entity>
      <a-obj-model id="cake" src="#tree-obj" mtl="#tree-mtl" position="0 0 .5" ></a-obj-model>
      <a-image src="#happy_holiday" width="2" height="2" rotation="-90 0 0"></a-image>
      <a-animation attribute="rotation" to="0 -360 0" dur="4000" repeat="indefinite" easing="linear"></a-animation>
    </a-entity>
    <a-entity>
      <a-obj-model id="santa" src="#santa-obj" mtl="#santa-mtl" position="1.5 1.5 0" scale="2 2 2"></a-obj-model>
      <a-animation attribute="rotation" to="360 -360 0" dur="4000" repeat="indefinite" easing="linear"></a-animation>
    </a-entity>
  
  </a-marker>

  <a-marker preset='kanji'>
   <a-sphere 
        color="#FFFFFF" 
        scale=".5 .5 .5" 
        position=".5 .5 0" 
        src="#aoimg">
        <a-animation attribute="position" to="0 1 .5" direction="alternate" dur="2000" repeat="indefinite"></a-animation>
        </a-animation>
        <a-animation attribute="rotation" to="-360 360 -360" dur="4000" repeat="indefinite" easing="linear"></a-animation>
    </a-sphere> 
    <a-sky src="/images/trees2.png"></a-sky>

  </a-marker>
  <a-entity camera></a-entity>
</a-scene>
